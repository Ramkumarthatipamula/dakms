<p id="notice"><%= notice %></p>

<h1>Institutes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Password</th>
      <th>Email</th>
      <th>Mobile number</th>
      <th>Phone number</th>
      <th>Address</th>
      <th>Designation</th>
      <th>Institute type</th>
      <th>Location</th>
      <th>Type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @institutes.each do |institute| %>
      <tr>
        <td><%= institute.name %></td>
        <td><%= institute.password %></td>
        <td><%= institute.email %></td>
        <td><%= institute.mobile_number %></td>
        <td><%= institute.phone_number %></td>
        <td><%= institute.address %></td>
        <td><%= institute.designation %></td>
        <td><%= institute.institute_type %></td>
        <td><%= institute.location %></td>
        <td><%= institute.type %></td>
        <td><%= link_to 'Show', institute %></td>
        <td><%= link_to 'Edit', edit_institute_path(institute) %></td>
        <td><%= link_to 'Destroy', institute, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Institute', new_institute_path %>
