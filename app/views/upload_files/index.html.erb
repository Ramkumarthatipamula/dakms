<p id="notice"><%= notice %></p>

<h1>Upload Files</h1>

<table>
  <thead>
    <tr>
      <th>Upload type</th>
      <th>Name</th>
      <th>Title</th>
      <th>Description</th>
      <th>Location</th>
      <th>Date</th>
      <th>Scientist name</th>
      <th>Scientist</th>
      <th>Status</th>
      <th>Archive</th>
      <th>Upload data</th>
      <th>Institute</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_files.each do |upload_file| %>
      <tr>
        <td><%= upload_file.upload_type %></td>
        <td><%= upload_file.name %></td>
        <td><%= upload_file.title %></td>
        <td><%= upload_file.description %></td>
        <td><%= upload_file.location %></td>
        <td><%= upload_file.date %></td>
        <td><%= upload_file.scientist_name %></td>
        <td><%= upload_file.scientist_id %></td>
        <td><%= upload_file.status %></td>
        <td><%= upload_file.archive %></td>
        <td><%= upload_file.upload_data_id %></td>
        <td><%= upload_file.institute_id %></td>
        <td><%= link_to 'Show', upload_file %></td>
        <td><%= link_to 'Edit', edit_upload_file_path(upload_file) %></td>
        <td><%= link_to 'Destroy', upload_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload File', new_upload_file_path %>
